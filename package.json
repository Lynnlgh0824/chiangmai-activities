{
  "name": "chiengmai-activities",
  "version": "2.0.0",
  "description": "清迈活动管理平台 - 整合版",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon server.cjs",
    "dev:client": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server.cjs",
    "check": "./quick-check.sh",
    "lint": "eslint src/**/*.{js,jsx} --max-warnings=0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "express",
    "react",
    "vite",
    "chiangmai",
    "activities"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "multer": "^2.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^4.0.18",
    "concurrently": "^8.2.0",
    "jsdom": "^27.4.0",
    "nodemon": "^3.1.0",
    "vite": "5.4.11",
    "vitest": "^4.0.18"
  }
}
